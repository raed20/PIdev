{% block body %}
    <h1>List of Commands</h1>

    <table>
        <thead>
            <tr>
                <th>ID</th>
                <th>Panier</th>
            </tr>
        </thead>
        <tbody>
            {% for order in orders %}
                <tr>
                    <td>{{ order.id }}</td>
                    <td>{{ order.panier }}</td>
                </tr>
            {% endfor %}
        </tbody>
    </table>
{% endblock %}
