{# templates/api/stock_quote.html.twig #}

<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Stock Quote Data</title>
</head>
<body>
    <h1>Stock Quote Data for </h1>
    <table>
        <thead>
            <tr>
                <th>Name</th>
                <th>Price</th>
                <th>24%</th>
            </tr>
        </thead>
        <tbody>
           {% for quote in stock_quote_data|slice (0, 10) %}
    <tr>
        <td>{{ quote['T'] }}</td>  {# Assuming 't' represents the date #}
        <td>{{ quote['v'] }}</td>  {# Assuming 'o' represents the opening price #}
        <td>{{ quote['vw'] }}</td>  {# Assuming 'h' represents the high price #}
		<td>
                    {% set companySymbol = quote['T'] %}
                    {% set logoData = unsplashApiService.getCompanyLogo(companySymbol) %}
                    <img src="{{ logoData['url'] }}" alt="{{ companySymbol }} Logo" width="100" height="100">
                </td>
    </tr>
{% endfor %}

        </tbody>
    </table>
</body>
</html>
